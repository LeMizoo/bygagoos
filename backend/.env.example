# Configuration du backend ByGagoos Ink

# Serveur
PORT=3001
NODE_ENV=development
CORS_ORIGIN=http://localhost:5173

# Base de données (à configurer plus tard)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=bygagoos_ink
DB_USER=postgres
DB_PASSWORD=password

# JWT (à configurer plus tard)
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_EXPIRES_IN=7d

# Uploads
MAX_FILE_SIZE=10485760 # 10MB
UPLOAD_PATH=./uploads